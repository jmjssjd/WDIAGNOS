<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   assembly="WDIAGNOS" 
                   namespace="WDIAGNOS.domain">
  <class name="Paciente">
    <id name="Id" column="ID_PACIENTE">
      <generator class="identity"/>
    </id>
    <property name="Apellidos" column="APELLIDOS"></property>
    <property name="Nombre" column="NOMBRE"></property>
    <property name="Telefono" column="TELEFONO"></property>
    <property name="FechaNacimiento" column="FECHA_NACIMIENTO"></property>
 </class>
         
</hibernate-mapping>

                   